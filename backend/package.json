{
  "name": "scpper-cn-backend",
  "version": "2.0.0",
  "description": "SCPPER-CN backend service with Prisma database integration",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "sync": "node --import tsx/esm src/cli/index.ts sync",
    "sync:full": "node --import tsx/esm src/cli/index.ts sync --full",
    "sync:test": "node --import tsx/esm src/cli/index.ts sync --test",
    "phase-a": "node --import tsx/esm src/cli/index.ts sync --phase a",
    "phase-b": "node --import tsx/esm src/cli/index.ts sync --phase b",
    "phase-c": "node --import tsx/esm src/cli/index.ts sync --phase c",
    "analyze": "node --import tsx/esm src/cli/sync.ts analyze",
    "analyze:incremental": "node --import tsx/esm -e \"import {analyzeIncremental} from './src/jobs/IncrementalAnalyzeJob.js'; analyzeIncremental();\"",
    "analyze:full": "node --import tsx/esm -e \"import {analyzeIncremental} from './src/jobs/IncrementalAnalyzeJob.js'; analyzeIncremental({forceFullAnalysis: true});\"",
    "analyze:init-history": "node --import tsx/esm -e \"import {analyzeIncremental} from './src/jobs/IncrementalAnalyzeJob.js'; analyzeIncremental({forceFullHistory: true, tasks: ['daily_aggregates']});\"",
    "analyze:complete-init": "node --import tsx/esm -e \"import {analyzeIncremental} from './src/jobs/IncrementalAnalyzeJob.js'; analyzeIncremental({forceFullAnalysis: true, forceFullHistory: true});\"",
    "query": "node --import tsx/esm src/cli/index.ts query",
    "query:help": "node --import tsx/esm src/cli/index.ts query --help",
    "query:stats": "node --import tsx/esm src/cli/index.ts query --stats",
    "query:rankings": "node --import tsx/esm src/cli/index.ts query --user-rank",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:clear": "node --import tsx/esm scripts/clear-data.ts",
    "check:alternate-title": "node --import tsx/esm scripts/check-alternate-title-sync.ts",
    "schema:fetch": "node --import tsx/esm scripts/fetch-graphql-schema.ts",
    "cleanup:high-count": "node --import tsx/esm scripts/cleanup-high-count-pages.ts",
    "audit:pageversion-validity": "node --import tsx/esm scripts/check-pageversion-validity-window.ts",
    "fix:pageversion-boundaries": "node --import tsx/esm scripts/fix-pageversion-boundaries.ts",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@types/uuid": "^10.0.0",
    "chalk": "^5.6.0",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "graphql-request": "^6.1.0",
    "graphql": "^16.9.0",
    "ora": "^8.2.0",
    "pretty-ms": "^9.2.0",
    "table": "^6.9.0",
    "tsx": "^4.19.1",
    "node-fetch": "^3.3.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "prisma": "^5.20.0",
    "typescript": "^5.6.2"
  }
}
